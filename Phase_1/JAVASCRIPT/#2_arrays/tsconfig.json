{
  // "include": ["src/**/*.ts"]
  "exclude": ["./js","node_modules"],
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "rootDir": "./ts",
    "outDir": "./js",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "strict": true,
    "alwaysStrict": true,
    "skipLibCheck": true
  }
}
